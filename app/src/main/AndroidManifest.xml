<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lister.bitmapproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".xfermode.XFerModeExampleActivity"/>
        <activity android:name=".color.BitmapColorActivity" />
        <activity android:name=".matrix.BitmapMatrixActivity" />
        <activity android:name=".xfermode.XFermodeActivity" />
        <activity android:name=".compress.CompressActivity" />
        <activity android:name=".crop.CropBitmapActivity" />
        <activity android:name=".joint.JointBitmapActivity" />
        <activity android:name=".crop.ChangeIconActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>